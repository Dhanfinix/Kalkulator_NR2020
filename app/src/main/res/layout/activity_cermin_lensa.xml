<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#004D40"
    android:paddingLeft="16dp"
    android:paddingTop="16dp"
    android:paddingRight="16dp"
    android:paddingBottom="16dp"
    tools:context=".CerminLensa">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#004D40">


            <Button
                android:id="@+id/btn_Info"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@id/tvJudul"
                android:layout_marginTop="10dp"
                android:background="@drawable/tombolhijau"
                android:gravity="center"
                android:text="Info kalkulator"
                android:textAllCaps="false"
                android:textColor="#000000"
                android:textSize="25sp"
                android:visibility="visible" />


            <Button
                android:id="@+id/btn_Hapus"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="310dp"

                android:background="@color/hapus"
                android:text="Hapus"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/bso"
                android:layout_width="123dp"
                android:layout_height="45dp"
                android:layout_below="@id/Tanya"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="10dp"
                android:background="#689F38"
                android:fontFamily="serif"
                android:text="So"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="italic" />

            <Button
                android:id="@+id/bsi"
                android:layout_width="123dp"
                android:layout_height="45dp"
                android:layout_below="@id/Tanya"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@id/bso"
                android:background="#689F38"
                android:fontFamily="serif"
                android:text="Si"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="italic" />


            <Button
                android:id="@+id/bfokus"
                android:layout_width="123dp"
                android:layout_height="45dp"
                android:layout_below="@id/Tanya"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@id/bsi"
                android:background="#689F38"
                android:fontFamily="serif"
                android:text="f"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="italic" />

            <Button
                android:id="@+id/btn_P"
                android:layout_width="123dp"
                android:layout_height="45dp"
                android:layout_below="@id/bfokus"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:fontFamily="serif"
                android:text="P"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="italic" />


            <Button
                android:id="@+id/btn_hi"
                android:layout_width="123dp"
                android:layout_height="45dp"
                android:layout_below="@id/bfokus"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/btn_P"
                android:background="#689F38"
                android:fontFamily="serif"
                android:text="hi"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="italic" />

            <Button
                android:id="@+id/btn_M"
                android:layout_width="123dp"
                android:layout_height="45dp"
                android:layout_below="@id/bfokus"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/btn_hi"
                android:background="#689F38"
                android:fontFamily="serif"
                android:text="M"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="italic" />

            <EditText
                android:id="@+id/in_f"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@id/v2"
                android:background="@color/colorWhite"
                android:gravity="center"
                android:hint="cm"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#00030F"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_so"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@id/so"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="cm"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#00030F"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_si"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/v2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/si"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="cm"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#00030F"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_r"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/v2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/r"
                android:background="@color/colorWhite"
                android:gravity="center"
                android:hint=" cm"
                android:inputType="number|numberDecimal"
                android:textColor="#00030F"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_I3"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/si"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/h"
                android:background="@color/colorWhite"
                android:gravity="center"
                android:hint="cm"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#00030F"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_P"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/si"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/P"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="dioptri"
                android:inputType="number|numberDecimal"
                android:textColor="#00030F"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvJudul"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"

                android:background="@drawable/roundedbutton"
                android:gravity="center"
                android:text="CERMIN DAN LENSA"
                android:textColor="#000000"
                android:textSize="30sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvtahu"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/btn_Info"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="10.2dp"
                android:background="#FFA000"
                android:gravity="center"
                android:text="Diketahui"
                android:textColor="#000000"
                android:textSize="25sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/v2"
                android:layout_width="83dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="10dp"
                android:background="#689F38"
                android:fontFamily="serif"
                android:gravity="center|right"
                android:inputType="number"
                android:text=" f = "
                android:textColor="#00030F"
                android:textSize="25sp"
                android:textStyle="italic" />


            <TextView
                android:id="@+id/so"
                android:layout_width="83dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@id/in_f"
                android:background="#689F38"
                android:fontFamily="serif"
                android:gravity="center|right"
                android:inputType="number"
                android:text="So = "
                android:textColor="#00030F"
                android:textSize="25sp"
                android:textStyle="italic" />


            <TextView
                android:id="@+id/si"
                android:layout_width="83dp"
                android:layout_height="45dp"
                android:layout_below="@id/v2"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:fontFamily="serif"
                android:gravity="center|right"
                android:inputType="number"
                android:text=" Si = "
                android:textColor="#00030F"
                android:textSize="25sp"
                android:textStyle="italic" />

            <TextView
                android:id="@+id/r"
                android:layout_width="83dp"
                android:layout_height="45dp"
                android:layout_below="@id/v2"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/in_si"
                android:background="#689F38"
                android:fontFamily="serif"
                android:gravity="center|right"
                android:inputType="number"
                android:text=" r = "
                android:textColor="#00030F"
                android:textSize="25sp"
                android:textStyle="italic" />

            <TextView
                android:id="@+id/h"
                android:layout_width="83dp"
                android:layout_height="45dp"
                android:layout_below="@id/si"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:fontFamily="serif"
                android:gravity="center|right"
                android:inputType="number"
                android:text=" h = "
                android:textColor="#00030F"
                android:textSize="25sp"
                android:textStyle="italic" />


            <TextView
                android:id="@+id/P"
                android:layout_width="83dp"
                android:layout_height="45dp"
                android:layout_below="@id/si"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/in_I3"
                android:background="#689F38"
                android:fontFamily="serif"
                android:gravity="center|right"
                android:inputType="number"
                android:text=" P = "
                android:textColor="#00030F"
                android:textSize="25sp"
                android:textStyle="italic" />

            <TextView
                android:id="@+id/Space1"
                android:layout_width="1dp"
                android:layout_height="20dp"
                android:layout_below="@id/btn_Hapus"
                android:layout_marginLeft="0dp" />

            <TextView
                android:id="@+id/Tanya"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/Space1"
                android:layout_marginLeft="0dp"
                android:background="#FFA000"
                android:gravity="center"
                android:text="Ditanyakan"
                android:textColor="#000000"
                android:textSize="25sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/Space4"
                android:layout_width="1dp"
                android:layout_height="20dp"
                android:layout_below="@id/btn_M"
                android:layout_marginLeft="0dp" />

            <TextView
                android:id="@+id/tvsolusi"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/Space4"
                android:layout_marginLeft="0dp"
                android:background="#FFA000"
                android:gravity="center"
                android:text="Jawaban"
                android:textColor="#000000"
                android:textSize="25sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/tampilan1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvsolusi"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="10dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textColor="#0C0C0C"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tampilan2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tampilan1"
                android:layout_marginLeft="0dp"
                android:background="#ffffff"
                android:gravity="center|start"
                android:textColor="#0C0C0C"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tampilan3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tampilan2"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tampilan4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tampilan3"
                android:layout_marginLeft="0dp"
                android:background="#ffffff"
                android:gravity="center|start"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tampilan5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tampilan4"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tampilan6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tampilan5"
                android:layout_marginLeft="0dp"
                android:background="#ffffff"
                android:gravity="center|start"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tampilan7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tampilan6"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tampilan8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tampilan7"
                android:layout_marginLeft="0dp"

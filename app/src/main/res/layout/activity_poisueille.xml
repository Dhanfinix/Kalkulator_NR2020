                android:textAllCaps="false"
                android:textSize="25sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btnQ"
                android:layout_width="73dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvTanya"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="12dp"
                android:background="#689F38"
                android:text="Q"
                android:textSize="20sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btnD"
                android:layout_width="72dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvTanya"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="12dp"
                android:layout_toRightOf="@id/btnQ"
                android:background="#689F38"
                android:text="D"
                android:textSize="20sp"
                android:textStyle="normal" />


            <Button
                android:id="@+id/btnP"
                android:layout_width="73dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvTanya"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="12dp"
                android:layout_toRightOf="@id/btnD"
                android:background="#689F38"
                android:text="P"
                android:textSize="20sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btnH"
                android:layout_width="73dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvTanya"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="12dp"
                android:layout_toRightOf="@id/btnP"
                android:background="#689F38"
                android:text="H"
                android:textSize="20sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btn_e"
                android:layout_width="73dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvTanya"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="12dp"
                android:layout_toRightOf="@id/btnH"
                android:background="#689F38"
                android:text="e"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textStyle="normal" />


            <EditText
                android:id="@+id/in_ro"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/ro"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=" kg/m3"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_H"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/H"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=" m"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_D"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/ro"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/D"

                android:layout_toRightOf="@id/Q"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=" l/s"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_e"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/L"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/e"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="poise"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_P"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/Q"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/P"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=" Pa"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#080809"
                android:textSize="25sp" />
            <EditText
                android:id="@+id/in_g"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/Q"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/g"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=" m/s2"
                android:inputType="number|numberDecimal|textCapCharacters"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvbab"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/roundedbutton"
                android:gravity="center"
                android:text="POISEUILLE"
                android:textColor="#000000"
                android:textSize="30sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvtahu"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/btn_Info"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="15dp"
                android:background="#FFA000"
                android:gravity="center"
                android:text="Diketahui"
                android:textColor="#000000"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/ro"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:background="#689F38"
                android:gravity="center|right"
                android:inputType="number"
                android:text=" ro = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/H"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/in_ro"
                android:background="#689F38"

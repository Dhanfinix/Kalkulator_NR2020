<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#004D40"
    android:paddingLeft="16dp"
    android:paddingTop="16dp"
    android:paddingRight="16dp"
    android:paddingBottom="16dp"
    tools:context=".SeriParalel"
    tools:ignore="ExtraText">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp">

            <TextView
                android:id="@+id/tvbab"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/roundedbutton"
                android:gravity="center"
                android:text="SERI PARALEL"
                android:textColor="#000000"
                android:textSize="30sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btn_Info"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@id/tvbab"
                android:layout_marginTop="6dp"
                android:background="@drawable/roundedbutton"
                android:gravity="center"
                android:text="Info kalkulator"
                android:textAllCaps="false"
                android:textColor="#000000"
                android:textSize="25sp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/tvtahu"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/btn_Info"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="15dp"
                android:background="#F57C00"
                android:gravity="center"
                android:text="Diketahui"
                android:textColor="#000000"
                android:textSize="25sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/r1"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  R1 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_r1"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/r1"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="ohm"
                android:inputType="number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />


            <TextView
                android:id="@+id/r2"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/in_r1"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  R2 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_r2"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/tvtahu"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/r2"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="ohm"
                android:inputType="number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />


            <TextView
                android:id="@+id/r3"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/r1"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  R3 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_r3"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/r1"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/r3"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="ohm "
                android:inputType="number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />


            <TextView
                android:id="@+id/C1"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/r1"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/in_r3"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  C1 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_C1"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/r1"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/C1"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="F"
                android:inputType="number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />


            <TextView
                android:id="@+id/C2"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/r3"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  C2 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_C2"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/r3"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/C2"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=" F "
                android:inputType="numberSigned|number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />


            <TextView
                android:id="@+id/C3"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/r3"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/in_C2"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  C3 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_C3"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/r3"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/C3"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=" F "
                android:inputType="numberSigned|number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/L1"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/C2"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  L1 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_L1"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/C2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/L1"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="H"
                android:inputType="number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />


            <TextView
                android:id="@+id/L2"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/C2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/in_L1"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  L2 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_L2"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/C2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/L2"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="H"
                android:inputType="number"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/L3"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/L2"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text="  L3 = "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_L3"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/L2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/L3"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint="H"
                android:inputType="number|numberDecimal"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/Lx"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_below="@id/L2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/in_L1"
                android:background="#689F38"
                android:gravity="right"
                android:inputType="number"
                android:text=" "
                android:textColor="#080809"
                android:textSize="25sp" />

            <EditText
                android:id="@+id/in_Lx"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_below="@id/L2"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/L2"
                android:background="@color/colorGrey"
                android:gravity="center"
                android:hint=""
                android:inputType="number"
                android:textColor="#080809"
                android:textSize="25sp" />


            <Button
                android:id="@+id/btn_Hapus"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/L3"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="15dp"
                android:background="@color/hapus"
                android:text="Hapus"
                android:textAllCaps="false"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/Tanya"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/btn_Hapus"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="15dp"
                android:background="#FFA000"
                android:fontFamily="serif"
                android:gravity="center"
                android:text="Ditanyakan"
                android:textColor="#080809"
                android:textSize="25sp" />


            <Button
                android:id="@+id/btn_Rs"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/Tanya"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="10dp"
                android:background="#689F38"
                android:text="R seri"
                android:textSize="25sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btn_Rp"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/Tanya"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@id/btn_Rs"
                android:background="#689F38"
                android:text="R par"
                android:textSize="25sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btn_Cs"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/Tanya"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@id/btn_Rp"
                android:background="#689F38"
                android:text=" C seri"
                android:textSize="25sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btn_Cp"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/btn_Rs"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="3dp"
                android:background="#689F38"
                android:text=" C par"
                android:textSize="25sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btn_Ls"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/btn_Rs"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/btn_Cp"
                android:background="#689F38"
                android:text="L seri"
                android:textSize="25sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/btn_Lp"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/btn_Rs"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/btn_Ls"
                android:background="#689F38"
                android:text="L par"
                android:textSize="25sp"
                android:textStyle="normal" />


            <TextView
                android:id="@+id/tvsolusi"
                android:layout_width="186dp"
                android:layout_height="45dp"
                android:layout_below="@id/btn_Lp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="15dp"
                android:background="#FFA000"
                android:fontFamily="serif"
                android:gravity="center"
                android:text="Jawaban"
                android:textColor="#080809"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvsolusi"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan1"
                android:layout_marginLeft="0dp"
                android:background="#ffffff"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan2"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan3"
                android:layout_marginLeft="0dp"
                android:background="#ffffff"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan4"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan5"
                android:layout_marginLeft="0dp"
                android:background="#ffffff"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan6"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan7"
                android:layout_marginLeft="0dp"
                android:background="#ffffff"
                android:gravity="center|start"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/tvtampilan9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvtampilan8"
                android:layout_marginLeft="0dp"
                android:background="@color/colorGrey"
                android:gravity="center|start"
                android:textSize="25sp" />


        </RelativeLayout>

    </ScrollView>


</RelativeLayout>
